<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EMSI_Corporation</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/EMSI_Corporation.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/mantenimiento.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/IndexRecepcion.css" asp-append-version="true" />

</head>
<body>
    <div class="wrapper w-100">
        @Html.Partial("_Sidebar")


        <div class="main-content">
            <main role="main" class="p-4">
                @RenderBody()
            </main>
        </div>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
